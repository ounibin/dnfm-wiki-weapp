{
  "pages": [
    "page/weapon/index",
    "page/armor/index",
    "page/jewelry/index",
    "page/card/index"
  ],
  "window": {
    "navigationBarTextStyle": "@navigationBarTextStyle",
    "navigationBarTitleText": "演示",
    "navigationBarBackgroundColor": "@navigationBarBackgroundColor",
    "backgroundColor": "@backgroundColor"
  },
  "tabBar": {
    "color": "@tabBarColor",
    "selectedColor": "@tabBarSelectedColor",
    "borderStyle": "@tabBarBorderStyle",
    "backgroundColor": "",
    "list": [
      {
        "pagePath": "page/weapon/index",
        "iconPath": "img/icon-weapon.png",
        "selectedIconPath": "img/icon-weapon-active.png",
        "text": "武器"
      },
      {
        "pagePath": "page/armor/index",
        "iconPath": "img/icon-armor.png",
        "selectedIconPath": "img/icon-armor-active.png",
        "text": "防具"
      },
      {
        "pagePath": "page/jewelry/index",
        "iconPath": "img/icon-jewelry.png",
        "selectedIconPath": "img/icon-jewelry-active.png",
        "text": "首饰"
      },
      {
        "pagePath": "page/card/index",
        "iconPath": "img/icon-card.png",
        "selectedIconPath": "img/icon-card-active.png",
        "text": "卡片"
      }
    ]
  },
  "networkTimeout": {
    "request": 10000,
    "connectSocket": 10000,
    "uploadFile": 10000,
    "downloadFile": 10000
  },
  "workers": "workers",
  "debug": false,
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于推荐附近玩家"
    }
  },
  "cloud": true,
  "sitemapLocation": "sitemap.json",
  "themeLocation": "app.theme.json",
  "darkmode": false,
  "resizable": true,
  "useExtendedLib": {
    "weui": false
  },
  "usingComponents": {
    "van-image": "@vant/weapp/image/index",
    "van-overlay": "@vant/weapp/overlay/index",
    "van-collapse-item": "@vant/weapp/collapse-item/index",
    "van-collapse": "@vant/weapp/collapse/index",
    "van-sidebar": "@vant/weapp/sidebar/index",
    "van-sidebar-item": "@vant/weapp/sidebar-item/index",
    "van-button": "@vant/weapp/button/index"
  },
  "frameset": true,
  "requiredBackgroundModes": [
    "audio",
    "location"
  ],
  "embeddedAppIdList": [
    "wxfdcee92a299bcaf1"
  ],
  "lazyCodeLoading": "requiredComponents"
